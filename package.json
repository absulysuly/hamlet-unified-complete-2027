{
    "name":  "copy-of-hamlet-social",
    "private":  true,
    "version":  "1.0.0",
    "type":  "module",
    "scripts":  {
                    "start":  "node server.js",
                    "backend:install":  "npm install --prefix backend",
                    "backend:build":  "npm run build --prefix backend",
                    "prisma:generate":  "npm run prisma:generate --prefix backend",
                    "postinstall":  "npm run backend:install && npm run prisma:generate && npm run backend:build",
                    "dev":  "vite",
                    "build":  "vite build",
                    "preview":  "vite preview",
                    "collect":  "node scripts/candidate-scraper.js",
                    "import":  "node scripts/data-importer.js import collected_candidates.json",
                    "schedule":  "node scripts/data-importer.js schedule",
                    "stats":  "node scripts/data-importer.js stats",
                    "storage:optimize":  "node scripts/storage-manager.js optimize",
                    "storage:archive":  "node scripts/storage-manager.js archive",
                    "storage:export":  "node scripts/storage-manager.js export",
                    "storage:analytics":  "node scripts/storage-manager.js analytics",
                    "qr:generate":  "node scripts/qr-generator.js generate",
                    "qr:analytics":  "node scripts/qr-generator.js analytics",
                    "collect:aggressive":  "node scripts/aggressive-collector.js",
                    "message:candidates":  "node scripts/personalized-messaging.js"
                },
    "dependencies":  {
                         "@google-cloud/storage":  "^7.17.2",
                         "@google/genai":  "^1.22.0",
                         "@prisma/client":  "^6.17.0",
                         "@tailwindcss/postcss":  "^4.1.14",
                         "axios":  "^1.12.2",
                         "bcryptjs":  "^3.0.2",
                         "cheerio":  "^1.1.2",
                         "cors":  "^2.8.5",
                         "csv-parser":  "^3.2.0",
                         "express":  "^5.1.0",
                         "googleapis":  "^161.0.0",
                         "mongodb":  "^6.20.0",
                         "next-auth":  "^4.24.11",
                         "nodemailer":  "^6.10.1",
                         "pdf-lib":  "^1.17.1",
                         "pdf-parse":  "^2.2.1",
                         "puppeteer":  "^24.23.0",
                         "react":  "^19.2.0",
                         "react-dom":  "^19.2.0",
                         "sqlite3":  "^5.1.7"
                     },
    "devDependencies":  {
                            "@types/node":  "^20.0.0",
                            "@types/react":  "^18.0.0",
                            "@types/react-dom":  "^18.0.0",
                            "@vitejs/plugin-react":  "^5.0.0",
                            "autoprefixer":  "^10.4.21",
